        <value>(function execute(inputs, outputs) {

/*
  Workspace Polling
    Finds any records not in an applied state then
    queries the Terraform Enterprise API for their status.
*/

// Input Variables
var tfRecord = inputs.tf_record;

// System Property variables for Terraform API
var terraformRecord = new tf_terraform_record();
var run             = new tf_run();
var workspace       = new tf_workspace();

var currentRun = workspace.getWorkspaceCurrentRun(tfRecord.workspace_name);
if (currentRun !== null) {
  var runID = currentRun.id;

  /* Data for the request content */
  var workspaceID = String(tfRecord.workspace_id);
  var runMesssage = "Triggered by scheduler";

  var content        = run.createRunRequestContent(workspaceID, runMesssage);
  var requestContent = JSON.stringify(content);

  /* HTTP REST request to Terraform API */
  var http = new tf_http("TF Runs", "Apply Run", requestContent);
	http.setParam("run_id", String(runID));
    http.execute();
  }
})(inputs, outputs);</value>
